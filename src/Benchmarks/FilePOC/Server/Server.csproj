<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="server.proto" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Include="..\..\..\RemoteWebViewService\RemoteStaticFiles\FileServiceImpl.cs" Link="FileServiceImpl.cs" />
	  <Compile Include="..\..\..\RemoteWebViewService\RemoteStaticFiles\FileStream.cs" Link="FileStream.cs" />
	  <Compile Include="..\..\..\RemoteWebViewService\RemoteStaticFiles\RemoteFileResolver.cs" Link="RemoteFileResolver.cs" />
	  <Compile Include="..\..\..\RemoteWebViewService\RemoteStaticFiles\RemoteFilesMiddleware.cs" Link="RemoteFilesMiddleware.cs" />
	  <Compile Include="..\..\..\RemoteWebViewService\RemoteStaticFiles\RemoteFilesMiddlewareExtensions.cs" Link="RemoteFilesMiddlewareExtensions.cs" />
	  <Compile Include="..\..\..\RemoteWebViewService\RemoteStaticFiles\RemoteFilesOptions.cs" Link="RemoteFilesOptions.cs" />
	  <Compile Include="..\..\..\RemoteWebViewService\RemoteStaticFiles\ServerFileSyncManager.cs" Link="ServerFileSyncManager.cs" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Grpc.AspNetCore" Version="2.65.0" />
		<PackageReference Include="Grpc.AspNetCore.Web" Version="2.65.0" />
		<PackageReference Include="Google.Protobuf" Version="3.28.0" />
		<PackageReference Include="Grpc.Core" Version="2.46.6" />
		<PackageReference Include="Grpc.Net.Client" Version="2.66.0-pre1" />
		<PackageReference Include="Grpc.Tools" Version="2.66.0">
		  <PrivateAssets>all</PrivateAssets>
		  <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		
	
		<Protobuf Include="..\..\..\Protos\webview.proto" GrpcServices="Server" Link="Protos\webview.proto" />
	</ItemGroup>
	<ItemGroup>
	  <Folder Include="publish\" />
	</ItemGroup>
</Project>
