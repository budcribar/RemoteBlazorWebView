<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net9-windows7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PublishAot>false</PublishAot>
		<PublishSingleFile>true</PublishSingleFile>
		<InvariantGlobalization>true</InvariantGlobalization>
		<RuntimeIdentifier>win-x64</RuntimeIdentifier>
		<SelfContained>true</SelfContained>
		<!--<IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>-->
		<DebugType Condition="'$(Configuration)' != 'Debug'">None</DebugType>
		<DebugSymbols Condition="'$(Configuration)' != 'Debug'">False</DebugSymbols>
		<ApplicationManifest>app.manifest</ApplicationManifest>
		<Platforms>x64</Platforms>
		<PlatformTargets>x64</PlatformTargets>
		<NoWarn>$(NoWarn);CA1416</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<EmbeddedResource Include="..\..\..\..\RemoteBlazorWebViewTutorial\RemoteBlazorWebViewTutorial.WpfApp\bin\publishEmbedded\RemoteBlazorWebViewTutorial.WpfApp.exe" Link="RemoteBlazorWebViewTutorial.WpfApp.exe" />
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="..\..\..\..\..\..\..\..\Certificates\DevCertificate_192.168.1.35.cer" Link="Resources\DevCertificate.cer">
	    <CopyToOutputDirectory>Never</CopyToOutputDirectory>
	  </EmbeddedResource>
	  <EmbeddedResource Include="Resources\.playwright.zip" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Playwright" Version="1.48.0" />
		<PackageReference Include="System.Management" Version="9.0.0-rc.2.24473.5" />
		<PackageReference Include="FluentAssertions" Version="6.12.1" />
		<PackageReference Include="System.Diagnostics.EventLog" Version="9.0.0-rc.2.24473.5" />
		<PackageReference Include="Google.Protobuf" Version="3.28.2" />
		<PackageReference Include="Grpc.Net.Client" Version="2.66.0" />
		<PackageReference Include="Grpc.Net.Client.Web" Version="2.66.0" />
		<ProjectReference Include="..\..\RemoteWebView\RemoteWebView.csproj" />
		<Protobuf Include="..\src\Protos\webview.proto" GrpcServices="Client" Link="Protos\webview.proto" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="publish\**\*" Exclude=" publish\Microsoft.Web.WebView2.Core.xml; publish\PeakSWC.RemoteWebView.pdb; publish\StressServer.exe; publish\.playwright\**\*">
			<Link>resources\%(RecursiveDir)%(Filename)%(Extension)</Link>
		</EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
	  <None Remove="Resources\.playwright.zip" />
	</ItemGroup>
	
	<ItemGroup>
	  <Compile Include="..\..\..\test\FileSyncServer.Tests\Utilities.cs" Link="Utilities.cs" />
	</ItemGroup>


	<ItemGroup>
	  <Folder Include="publish\" />
	</ItemGroup>


</Project>
